  "devDependencies": {
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
